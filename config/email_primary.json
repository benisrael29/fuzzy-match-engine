{
  "source1": "data/people_master.csv",
  "source2": "data/people_new.csv",
  "output": "results/email_matches.csv",
  "match_config": {
    "columns": [
      {
        "source1": "email",
        "source2": "email_address",
        "weight": 0.8
      },
      {
        "source1": "first_name",
        "source2": "fname",
        "weight": 0.1
      },
      {
        "source1": "last_name",
        "source2": "lname",
        "weight": 0.1
      }
    ],
    "threshold": 0.90,
    "undecided_range": 0.03,
    "return_all_matches": false
  }
}

